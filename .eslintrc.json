{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-undef": "error",
        "no-unused-vars": "error",
        "prefer-template": "error"
    }
}
